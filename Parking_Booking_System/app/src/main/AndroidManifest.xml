<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.muhammadusama.parking_booking_system">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_car"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_car"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SignUpActivity"
            android:label="@string/sign_up" />
        <activity
            android:name=".UserActivity"
            android:label="@string/user" />
        <activity
            android:name=".AdminActivity"
            android:label="@string/admin" />
        <activity
            android:name=".AreasActivity"
            android:label="@string/areas" />
        <activity
            android:name=".BookingActivity"
            android:label= "@string/book"/>
        <activity
            android:name=".AdminViewActivity"
            android:label="View" />
        <activity
            android:name=".ViewUsersActivity"
            android:label="Users" />
        <activity
            android:name=".CancelBookingActivity"
            android:label="Cancel Booking" />
        <activity
            android:name=".ViewBookingsActivity"
            android:label="@string/bookings" />
        <activity
            android:name=".UserFeedbackActivity"
            android:label="@string/feedback" />
        <activity android:name=".FeedbacksListActivity"
            android:label="@string/feedback"></activity>
    </application>

</manifest>